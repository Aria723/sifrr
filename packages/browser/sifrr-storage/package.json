{
  "name": "@sifrr/storage",
  "version": "0.1.0",
  "description": "Frontend key-value(JSON to JSON) storage library",
  "licence": "MIT",
  "main": "src/browserstorage.js",
  "browser": "dist/browserstorage.min.js",
  "browserslist": [
    "chrome >= 55",
    "safari >= 10.1",
    "opera >= 42",
    "firefox >= 53"
  ],
  "scripts": {
    "test": "mocha --timeout 10000",
    "jtest": "MOCHA_FILE=./test/test-results.xml mocha -R mocha-junit-reporter -R spec --timeout 10000",
    "browser-test": "mocha --timeout 10000 test/browser.js test/storages.js",
    "unit-test": "mocha test/test.js",
    "rollup": "./node_modules/.bin/rollup -c",
    "docs": "./node_modules/.bin/jsdoc src/browserstorage.js -d docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aadityataparia/browserStorage.git"
  },
  "keywords": [
    "indexeddb",
    "localstorage",
    "storage",
    "websql",
    "json"
  ],
  "author": "Aaditya Taparia",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aadityataparia/browserStorage/issues"
  },
  "homepage": "https://github.com/aadityataparia/browserStorage",
  "dependencies": {},
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.7.0",
    "browserslist": "^4.1.0",
    "chai": "^4.1.2",
    "fast-async": "^6.3.8",
    "file-url": "^2.0.2",
    "jsdoc": "^3.5.5",
    "mocha": "^5.2.0",
    "mocha-junit-reporter": "^1.18.0",
    "puppeteer": "^1.11.0",
    "repl": "^0.1.3",
    "rollup": "^0.64.1",
    "rollup-plugin-babel": "^3.0.7",
    "rollup-plugin-cleanup": "^3.0.0",
    "rollup-plugin-commonjs": "^9.1.5",
    "rollup-plugin-eslint": "^5.0.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-terser": "^1.0.1"
  }
}
