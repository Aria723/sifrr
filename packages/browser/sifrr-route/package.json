{
  "name": "@sifrr/route",
  "version": "0.0.1-alpha",
  "description": "History API based Routing library for sifrr in browser.",
  "licence": "MIT",
  "main": "src/sifrr.route.js",
  "browser": "dist/sifrr.route.js",
  "browserslist": [
    "chrome >= 55",
    "safari >= 10.1",
    "opera >= 42",
    "firefox >= 53"
  ],
  "scripts": {
    "test": "mocha --timeout 10000 test/**/*.test.js",
    "jtest": "MOCHA_FILE=../../../test-results/sifrr-route/results.xml yarn test -R mocha-junit-reporter",
    "browser-test": "mocha --timeout 5000 test/browser/*.test.js",
    "unit-test": "mocha test/unit/*.test.js",
    "rollup": "./node_modules/.bin/rollup -c",
    "test-rollup": "cd test/public && ../../node_modules/.bin/rollup -c",
    "test-server-only": "node ../../../test-server.js -d ./packages/browser/sifrr-route/test/public -p 1111",
    "test-server": "yarn test-rollup && yarn test-server-only"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sifrr/sifrr.git"
  },
  "keywords": [
    "sifrr",
    "route"
  ],
  "author": "Sifrr",
  "bugs": {
    "url": "https://github.com/sifrr/sifrr/issues"
  },
  "homepage": "https://github.com/sifrr/sifrr",
  "dependencies": {},
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.7.0",
    "browserslist": "^4.1.0",
    "chai": "^4.1.2",
    "express": "^4.16.4",
    "file-url": "^2.0.2",
    "mocha": "^5.2.0",
    "mocha-junit-reporter": "^1.18.0",
    "puppeteer": "^1.11.0",
    "repl": "^0.1.3",
    "rollup": "^0.64.1",
    "rollup-plugin-babel": "^3.0.7",
    "rollup-plugin-cleanup": "^3.0.0",
    "rollup-plugin-commonjs": "^9.1.5",
    "rollup-plugin-eslint": "^5.0.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-terser": "^1.0.1"
  },
  "files": [
    "bin",
    "dist",
    "src"
  ]
}
